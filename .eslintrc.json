{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "extends": ["standard", "jquery"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "lines-around-comment": "off",
    "camelcase": "off",
    "no-unused-vars": "warn"
  }
}
